#include<stdio.h>
int main()
{
   int mat1[10][10], mat2[10][10], mat3[10][10], i, j;
 
   printf("Enter First Matrix:\n");
   for (i = 0; i < 6; i++)
   {
      for (j = 0; j < 6; j++)
      {
         scanf("%d", &mat1[i][j]);
      }
   }
 
   printf("\nEnter Second Matrix:\n");
   for (i = 0; i < 6; i++)
   {
      for (j = 0; j < 6; j++)
      {
         scanf("%d", &mat2[i][j]);
      }
   }
 
   printf("The First Matrix: \n");
   for (i = 0; i < 6; i++)
   {
      for (j = 0; j < 6; j++)
      {
         printf(" %d ", mat1[i][j]);
      }
      printf("\n");
   }
 
   printf("The Second Matrix: \n");
   for (i = 0; i < 6; i++)
   {
      for (j = 0; j < 6; j++)
      {
         printf(" %d ", mat2[i][j]);
      }
      printf("\n");
   }
   for(i=0;i<6;i++)
   {
   	for(j=0;j<6;j++)
   	mat3[i][j]=mat1[i][j]+mat2[i][j];
   }
    printf("\nAddition Of Two Matrices : \n");
   for (i = 0; i < 6; i++)
   {
      for (j = 0; j < 6; j++)
      {
         printf(" %d ", mat3[i][j]);
      }
      printf("\n");
   }
 
   return (0);
}
